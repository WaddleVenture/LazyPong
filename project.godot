[file_customization]

folder_colors={
"res://assets/": "red",
"res://datas/": "orange",
"res://resources/": "green",
"res://scenes/": "teal",
"res://scripts/": "blue"
}

[rendering]

textures/canvas_textures/default_texture_filter=0
